<?xml version="1.0" encoding="utf-8"?>
<extension type="System" version="1.0" extends="ExtensionInstancesTable">
    <name>SYSTEM_MENUS_NAME</name>
    <author>Molajo Team</author>
    <create_date>August 2012</create_date>
    <copyright>Copyright (C) 2012 Individual Molajo Contributors. All rights reserved.</copyright>
    <license>GNU GPL v 2, or later and MIT, see License folder</license>
    <author_email>collaborate@molajo.org</author_email>
    <author_url>http://molajo.org</author_url>
    <version>1.0</version>
    <description>SYSTEM_MENUS_DESCRIPTION</description>
    <model
            name="Menus"
            catalog_type_id="1300"
            extension_instance_id="8"
            extends="ExtensionInstances">
        <table>
            <joins>
                <join model="Extensions"
                      alias="content_catalog_type_extension"
                      select="id,name"
                      jointo="id"
                      joinwith="extension_id"/>
                <join model="CatalogTypes"
                      alias="content_catalog_types"
                      select="id,type,title,extension_instance_id,model_name,primary_category_id,source_table,slug,routable"
                      jointo="model_name"
                      joinwith="content_catalog_type_extension.name"/>
            </joins>
        </table>
    </model>

</extension>
