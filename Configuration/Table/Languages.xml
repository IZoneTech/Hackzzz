<?xml version="1.0" encoding="utf-8"?>
<model
        name="Languages"
        table="#__extension_instances"
        primary_key="id"
        name_key="title"
        primary_prefix="a"
        get_customfields="1"
        get_item_children="0"
        use_special_joins="1"
        check_view_level_access="0"
        process_plugins="1"
        catalog_type_id="1100"
        use_pagination="1"
        data_source="JDatabase">

    <table>
        <fields>
            <include name="FieldsExtensionInstance"/>
        </fields>
        <joins>
            <include name="JoinsExtensionInstance"/>
        </joins>
        <foreignkeys>
            <include name="ForeignKeysExtensionInstance"/>
        </foreignkeys>
        <children>
            <include name="ChildrenExtensionInstance"/>
        </children>
        <plugins>
            <include name="PluginsAdmin"/>
            <include name="PluginsExtensionInstance"/>
        </plugins>
    </table>
    <customfields>
        <customfield name="metadata">
            <include name="Metadata"/>
        </customfield>
        <customfield name="parameters">
            <include name="ParametersLanguage"/>
            <include name="ParametersCache"/>
        </customfield>
    </customfields>
</model>
