{
    "name": "molajo/molajo",
    "description": "Molajo Standard Distribution",
    "type": "application",
    "keywords": ["Molajo", "Distribution", "Application", "CommonApi"],
    "homepage": "http://github.com/Molajo/Molajo",
    "license": "MIT",
    "version": "1.0.0",

    "authors": [
        {
            "name": "Amy Stephen",
            "email": "AmyStephen@Molajo.org",
            "homepage": "http://AmyStephen.com"
        }
    ],

    "support": {
        "event": "hello@molajo.org",
        "irc": "irc://irc.freenode.org/molajo"
    },

    "require": {
        "php": ">=5.4",
        "molajo/application": "dev-master",
        "molajo/foundation5": "dev-master"
